   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Array : [10 20 10 30 20 40]\n",
      "Unique elements : [10 20 30 40]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "arr=np.array([10,20,10,30,20,40])\n",
    "print(\"Array :\",arr)\n",
    "print(\"Unique elements :\",np.unique(arr))"
   ]
